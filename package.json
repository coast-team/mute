    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "libp2p": "0.37.3",
    "loglevel": "^1.8.1",
    "material-icons": "^1.13.12",
    "nanoid": "^4.0.1",
    "netflux": "^4.3.1",
    "np2-ui-auth": "harmanpa/ng2-ui-auth#aa965bc5a8982ab51a26deb4e15b04fde55cf8c9",
    "protobufjs": "^6.8.8",
    "run-script-os": "^1.1.6",
    "rxjs": "^6.6.6",

{
  "name": "mute",
  "version": "0.1.1",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "prestart": "pm2 gracefulReload process.yml",
    "start": "ng serve --host 0.0.0.0 --disable-host-check --watch",
    "prebuild": "sed -i -- \"s/'.*'/'$(git log -n 1 --pretty=format:'%H')'/g\" src/lastcommithash.ts",
    "build": "ng build --prod",
    "postbuild": "sed -i -- \"s/'.*'/''/g\" src/lastcommithash.ts && zopfli dist/*.js dist/*.css dist/*.html",
    "builddev": "npm run build -- --env ovhdev",
    "postbuilddev": "tar -cvf dist/mute.tar.gz -C dist . && scp dist/mute.tar.gz ovhmute:/root/container/mute",
    "lint": "ng lint --type-check",
    "test": "ng test",
    "e2e": "ng e2e",
    "proto": "pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert -o src/app/doc/network/message_pb.js src/app/doc/network/message.proto && pbts -o src/app/doc/network/message_pb.d.ts src/app/doc/network/message_pb.js && pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert -o src/app/doc/editor/cursor/cursor_pb.js src/app/doc/editor/cursor/cursor.proto && pbts -o src/app/doc/editor/cursor/cursor_pb.d.ts src/app/doc/editor/cursor/cursor_pb.js",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "cz": "git-cz",
    "build-desktop": "nativefier --name 'Mute' 'https://www.coedit.re' ./dist-desktop"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>",
    "Quentin Tardivon",
    "Camille Menou",
    "Yann Prono"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.0.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/flex-layout": "2.0.0-beta.12",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@angular/service-worker": "^5.2.0",
    "@coast-team/mnemonicjs": "^1.0.0",
    "@types/text-encoding": "0.0.32",
    "bragi-browser": "0.0.12",
    "codemirror": "~5.28.0",
    "core-js": "^2.5.3",
    "hammerjs": "^2.0.8",
    "hypermd": "git+https://github.com/coast-team/HyperMD.git",
    "jio": "git+https://github.com/Nexedi/jio.git#81cb08d",
    "loglevel": "^1.6.1",
    "material-design-icons": "^3.0.1",
    "mathjax": "^2.7.1",
    "mute-core": "^0.4.0-3",
    "mute-structs": "^0.4.0-2",
    "netflux": "^2.1.0",
    "ng2-ui-auth": "^7.0.2",
    "normalize.css": "^7.0.0",
    "protobufjs": "^6.8.4",
    "rxjs": "^5.5.6",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/codemirror": "latest",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/mathjax": "0.0.32",
    "@types/node": "^8.5.8",
    "codelyzer": "^4.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "pm2": "^2.9.1",
    "protractor": "~5.1.2",
    "sigver": "^17.0.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.5.3",
    "validate-commit-msg": "^2.14.0"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mute": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["src/assets"]
            },
            "assets": ["src/assets/images", "src/assets/fonts", "src/manifest.json"],
            "styles": ["src/styles.scss"],
            "scripts": [
              "node_modules/loglevel/dist/loglevel.min.js",
              "src/assets/rsvp-2.0.4.min.js",
              "src/assets/jio-latest.js",
              "node_modules/@multiavatar/multiavatar/multiavatar.min.js"
            ],
            "allowedCommonJsDependencies": [
              "codemirror/mode/javascript/javascript",
              "codemirror/mode/gfm/gfm",
              "@toast-ui/editor",
              "@stomp/rx-stomp"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "debug": {
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "serviceWorker": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": false,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mute:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mute:build:production"
            },
            "debug": {
              "browserTarget": "mute:build:debug"
            },
            "ovhdev": {
              "browserTarget": "mute:build:ovhdev"
            },
            "ovhprod": {
              "browserTarget": "mute:build:ovhprod"
            },
            "demo": {
              "browserTarget": "mute:build:demo"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mute:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": ["node_modules/loglevel/dist/loglevel.min.js", "src/assets/rsvp-2.0.4.min.js", "src/assets/jio-latest.js"],
            "styles": ["src/styles.scss"],
            "assets": ["src/assets/images", "src/assets/fonts", "src/manifest.json"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "mute-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "mute:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "mute",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "mute",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "mute"
    }
  }
}
